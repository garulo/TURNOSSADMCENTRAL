<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>InicoSesion</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.InicoSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesion</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.CerrarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarOficina</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.AgregarOficina</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarOficina</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.EliminarOficina</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarOficina</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.EditarOficina</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarServicio</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.AgregarServicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarServicio</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.EliminarServicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarServicio</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.EditarServicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarUsuario</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.AgregarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarUsuario</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.EliminarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarUsuario</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.EditarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarTipoServicio</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.AgregarTipoServicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarTipoServicio</servlet-name>
        <servlet-class>com.sadm.turnos.servlet.EliminarTipoServicio</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InicoSesion</servlet-name>
        <url-pattern>/inicosesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesion</servlet-name>
        <url-pattern>/cerrarsesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarOficina</servlet-name>
        <url-pattern>/agregaroficina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarOficina</servlet-name>
        <url-pattern>/eliminaroficina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarOficina</servlet-name>
        <url-pattern>/editaroficina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarServicio</servlet-name>
        <url-pattern>/agregarservicio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarServicio</servlet-name>
        <url-pattern>/eliminarservicio</url-pattern>
