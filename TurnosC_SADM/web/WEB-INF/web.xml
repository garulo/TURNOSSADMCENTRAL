    <servlet-mapping>
        <servlet-name>AgregarTipoServicio</servlet-name>
        <url-pattern>/agregartipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarTipoServicio</servlet-name>
        <url-pattern>/eliminartipo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
